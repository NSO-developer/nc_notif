        }
        container config {
          description
            "Config. container for assigning APs to managers.";
          uses ncs_rpc_notif_assigned-ap-managers-config;
        }
        container state {
          config false;
          description
            "State container for APs assigned to managers.";
          uses ncs_rpc_notif_assigned-ap-managers-config;
          uses ncs_rpc_notif_assigned-ap-managers-state;
