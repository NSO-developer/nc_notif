  augment "/ncs:devices/ncs:device/ncs:live-status" {
    uses live_ncs_access-points-top {
      refine access-points {
        tailf:ncs-device-type "netconf";
        tailf:display-when "not(../../ncs:module) or ../../ncs:module[ncs:name='openconfig-access-points'] or (../../ncs:live-status-protocol[not(ncs:capability)]) or (../../ncs:live-status-protocol/ncs:capability/ncs:module[.='openconfig-access-points'])";
      }
